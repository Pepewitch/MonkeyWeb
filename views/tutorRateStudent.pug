extends htmlTemplate
block head
    -const pageTitle = "Rate Students"
    script(src="js/typeahead.bundle.min.js")
    script(src="js/tutorRateStudent.js")
    style.
        h2{
            margin: 0 0 0 0;
        }
        .tt-input{
            width: 10vw;
            font-size: 20px;
        }
        .tt-suggestion{
            font-size: 16px;
        }
        .tt-highlight{
            font-size: 16px;
        }
        .tt-menu{
            width: 10vw;
            border: 3px solid blue;
        }
block body
    .container
        .col-md-4
            h2 Type Name of Student
        #search-box
            input.typeahead(type="text" placeholder="Name of Student")